{
    "name": "calva-lib",
    "displayName": "The Calva Library",
    "description": "A common library for the Calva Family of extensions",
    "version": "0.0.1",
    "keywords": [
        "Clojure",
        "ClojureScript",
        "EDN",
        "Formatting",
        "Pretty"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/BetterThanTomorrow/calva-lib.git"
    },
    "scripts": {
